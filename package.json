{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_MOCKING=true vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "cross-env VITE_MOCKING=true vitest run --coverage",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook --coverage",
    "build-storybook": "storybook build",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test-storybook\"",
    "build-tailwind": "npx tailwindcss -i ./src/style.in.css -o ./src/style.css",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/addon-coverage": "^1.0.1",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@storybook/test-runner": "^0.16.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.5",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "daisyui": "^4.7.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "msw": "^2.2.8",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react-remove-attr": "^0.0.6",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.3",
    "typescript": "5.1.3",
    "typescript-eslint": "^7.1.1",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.3.1",
    "wait-on": "^7.2.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
