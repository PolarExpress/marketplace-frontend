{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",

  "plugins": ["prettier-plugin-jsdoc"],
  "jsdoc-comment-line-strategy": "multiline",
  "jsdoc-description-with-dot": true,
  "jsdoc-prefer-code-fences": true,
  "jsdoc-separate-tag-groups": true
}
